// schema.prisma

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model survey {
  id           Int      @id @default(autoincrement())
  q1           Int
  q2           Int
  q3           Int
  q4           Int
  q5           Int
  q6           Int
  q7           Int
  q8           Int
  q9           Int
  q10          Int
  summary      Int
  age          String?
  education    String?
  ethnic       String?
  location     String?
  migrant      String?
  orientation  String?
  feedback     String?
  feedbackLike Boolean?
  createdAt    DateTime @default(now())
}
